{
  "manifest_version": 2,
  "name": "Learn Languages",
  "version": "1.0",
  "description": "I want to learn languages",
  "permissions": ["activeTab", "storage", "tabs", "*://*.youtube.com/*"],
  "browser_action": {
    "default_title": "Learn Languages",
    "browser_style": true,
    "default_icon": {
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["script.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
