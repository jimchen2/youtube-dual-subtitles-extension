{
    "manifest_version": 2,
    "name": "Learn Russian",
    "version": "1.0",
    "description": "I want to learn Russian",
    "permissions": [
      "activeTab",
      "storage",
      "tabs",
      "*://*.youtube.com/*"
    ],
    "browser_action": {
      "default_title": "Learn Russian",
      "browser_style": true,
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["inject.js"]
      }
    ],
    "icons": {
      "128": "icon.png"
    }
  }
  